{
    "name": "mpyw/unique-violation-detector",
    "description": "Detect primary/unique key or constraint violation errors from PDOException.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["database", "duplicate", "constraint"],
    "autoload": {
        "psr-4": {
            "Mpyw\\UniqueViolationDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\UniqueViolationDetector\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=11.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
